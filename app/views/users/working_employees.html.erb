<% provide(:title, 'Working Employees') %>
<h1>出勤中社員一覧</h1>

<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>社員番号</th>
            <th>名前</th>
          </tr>
        </thead>
        <tbody>
          <% @working_employees.each do |employee| %>
            <tr>
              <td></td>
              <td><%= employee.name %></td>
            </tr>    
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>